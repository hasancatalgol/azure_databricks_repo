{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "databricks-training-df"
		},
		"formula1_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'formula1_ls'"
		},
		"hasanctestazuredbls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hasanctestazuredbls'"
		},
		"hasanctestazuredblegacyls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'hasanctestazuredblegacyls'"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://hasanc-databricks-kv.vault.azure.net/"
		},
		"formula1_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dbformula1dldldl.dfs.core.windows.net/"
		},
		"AzureSqlDatabase720_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/fcabc251-87e8-4d92-ae25-3b93a086675f/resourceGroups/training-dataengineering-rg/providers/Microsoft.Sql/servers/hasanc-test-server"
		},
		"AzureSqlDatabase720_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"hasanctestazuredbls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hasanc-test-server.database.windows.net"
		},
		"hasanctestazuredbls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "hasanc-test-azuredb"
		},
		"hasanctestazuredbls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hasanc"
		},
		"hasanctestazuredblegacyls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "hasanc-test-server.database.windows.net"
		},
		"hasanctestazuredblegacyls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "hasanc-test-azuredb"
		},
		"hasanctestazuredblegacyls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "hasanc"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "databricks-training-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"formula1_ls_accountKey": {
						"value": "[parameters('formula1_ls_accountKey')]"
					},
					"hasanctestazuredbls_password": {
						"value": "[parameters('hasanctestazuredbls_password')]"
					},
					"hasanctestazuredblegacyls_password": {
						"value": "[parameters('hasanctestazuredblegacyls_password')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"formula1_ls_properties_typeProperties_url": {
						"value": "[parameters('formula1_ls_properties_typeProperties_url')]"
					},
					"AzureSqlDatabase720_properties_privateLinkResourceId": {
						"value": "[parameters('AzureSqlDatabase720_properties_privateLinkResourceId')]"
					},
					"AzureSqlDatabase720_properties_groupId": {
						"value": "[parameters('AzureSqlDatabase720_properties_groupId')]"
					},
					"hasanctestazuredbls_properties_typeProperties_server": {
						"value": "[parameters('hasanctestazuredbls_properties_typeProperties_server')]"
					},
					"hasanctestazuredbls_properties_typeProperties_database": {
						"value": "[parameters('hasanctestazuredbls_properties_typeProperties_database')]"
					},
					"hasanctestazuredbls_properties_typeProperties_userName": {
						"value": "[parameters('hasanctestazuredbls_properties_typeProperties_userName')]"
					},
					"hasanctestazuredblegacyls_properties_typeProperties_server": {
						"value": "[parameters('hasanctestazuredblegacyls_properties_typeProperties_server')]"
					},
					"hasanctestazuredblegacyls_properties_typeProperties_database": {
						"value": "[parameters('hasanctestazuredblegacyls_properties_typeProperties_database')]"
					},
					"hasanctestazuredblegacyls_properties_typeProperties_userName": {
						"value": "[parameters('hasanctestazuredblegacyls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}