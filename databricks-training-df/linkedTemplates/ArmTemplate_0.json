				"typeProperties": {
					"server": "[parameters('hasanctestazuredbls_properties_typeProperties_server')]",
					"database": "[parameters('hasanctestazuredbls_properties_typeProperties_database')]",
					"encrypt": "mandatory",
					"trustServerCertificate": false,
					"authenticationType": "SQL",
					"userName": "[parameters('hasanctestazuredbls_properties_typeProperties_userName')]",
					"password": {
						"type": "SecureString",
						"value": "[parameters('hasanctestazuredbls_password')]"
					},
					"sqlVersion": "SQL Server 2019"
				},
				"connectVia": {
					"referenceName": "ManagedIR",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/ManagedIR')]"
			]
		},
